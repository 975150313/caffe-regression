name: "fkp_net"
layers {
  name: "MyData"
  type: HDF5_DATA
  top: "data"
  top: "label"
  hdf5_data_param {
    source: "data/kaggle_fkp_data/train_data_list.txt"
    batch_size: 128
    shuffle: true
